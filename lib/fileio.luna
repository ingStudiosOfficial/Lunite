class FileUtils {
    let path = ""
    
    func init(p) {
        this.path = p
    }

    func read() {
        return File.read(this.path)
    }

    func write(content) {
        File.write(this.path, content)
    }

    func exists() {
        return File.exists(this.path)
    }
}